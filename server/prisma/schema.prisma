datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  branches  Branch[] @relation(name: "branches")
  createdAt DateTime @default(now())
  email     String   @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     String[]
  test      Branch[] @relation(name: "BranchTestUserTest")
  updatedAt DateTime @updatedAt
  userId    String?
  username  String   @unique
}

model Branch {
  address           String?
  branchCode        String?
  branchmanagerid   User?    @relation(name: "branches", fields: [branchmanageridId], references: [id])
  branchmanageridId String?
  branchName        String?
  createdAt         DateTime @default(now())
  id                String   @id @default(cuid())
  location          String?
  status            String?
  test              User?    @relation(name: "BranchTestUserTest", fields: [testId], references: [id])
  testId            String?
  updatedAt         DateTime @updatedAt
}

model Product {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  name        String
  updatedAt   DateTime @updatedAt
}

model Role {
  createdAt DateTime @default(now())
  fiadminid Json?
  id        String   @id @default(cuid())
  name      String?
  roleId    Int?
  updatedAt DateTime @updatedAt
  userid    Json?
}

model Fi {
  address   String
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String?  @unique
  updatedAt DateTime @updatedAt
}

model Testing {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  lastName  String?
  testName  String?
  updatedAt DateTime @updatedAt
}

model Demo {
  createdAt DateTime @default(now())
  demoname  String?
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model Feature {
  createdAt   DateTime @default(now())
  featureName String?
  id          String   @id @default(cuid())
  updatedAt   DateTime @updatedAt
}

model Expense {
  createdAt    DateTime @default(now())
  exprensename String?
  id           String   @id @default(cuid())
  updatedAt    DateTime @updatedAt
}